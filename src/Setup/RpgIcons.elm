mobsterIcon : RpgRole -> Html.Html msg
mobsterIcon role =
    case role of
        RpgRole.Driver ->
            driverIcon level1Color

        RpgRole.Navigator ->
            navigatorIcon level1Color

        RpgRole.Researcher ->
            researcherIcon level2Color

        RpgRole.Sponsor ->
            sponsorIcon level2Color

        RpgRole.Mobber ->
            mobberIcon level1Color


level1Color : Style.Color
